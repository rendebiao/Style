<resources>

    <style name="BaseTheme" parent="Theme.AppCompat.Light.DarkActionBar" />

    <style name="AppCustomTheme" parent="BaseTheme">
        <item name="colorPrimary">?attr/themeColor</item>
        <item name="colorPrimaryDark">?attr/themeColor</item>
        <item name="colorAccent">?attr/themeColor</item>

        <item name="android:scrollbarSize">3dp</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/theme_white</item>

        <!--以下为自定义属性-->
        <!--主题色-->
        <item name="themeColor">@color/theme_color</item>
    </style>

    <style name="AppCustomTheme.Translucent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppCustomTheme.Fullscreen">
        <item name="android:windowFullscreen">true</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
    </style>

    <style name="CoreTheme" parent="AppCustomTheme">
        <!--        <item name="colorPrimaryDark">@color/theme_transparent</item>-->
    </style>

    <style name="CoreTheme.Translucent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="CoreTheme.Fullscreen">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowLayoutInDisplayCutoutMode">shortEdges</item>
    </style>

    <style name="CoreTheme.Translucent.Fullscreen">
        <item name="android:windowFullscreen">true</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
    </style>

